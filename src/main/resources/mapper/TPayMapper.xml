        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="generator.mapper.TPayMapper">

    <resultMap id="BaseResultMap" type="generator.domain.TPay">
